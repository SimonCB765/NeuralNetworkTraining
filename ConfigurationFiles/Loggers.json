{
  "disable_existing_loggers": false,
  "formatters": {
    "default": {
      "format": "%(name)s\t%(levelname)s\t%(message)s"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "formatter": "default",
      "level": "CRITICAL"
    },
    "file": {
      "class": "logging.FileHandler",
      "filename": "placeholder",
      "formatter": "default",
      "level": "DEBUG"
    }
  },
  "incremental": false,
  "loggers": {
    "__main__": {
      "handlers": ["console", "file"],
      "level": "DEBUG"
    },
    "DataProcessing": {
      "handlers": ["console", "file"],
      "level": "DEBUG"
    },
    "Libraries": {
      "handlers": ["console", "file"],
      "level": "DEBUG"
    },
    "Network": {
      "handlers": ["console", "file"],
      "level": "DEBUG"
    },
    "Utilities": {
      "handlers": ["console", "file"],
      "level": "DEBUG"
    }
  },
  "root": {},
  "version": 1
}